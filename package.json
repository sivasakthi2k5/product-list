{
  "name": "product-list",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build:bundle": "NODE_ENV=production webpack",
    "start": "NODE_ENV=development webpack-dev-server",
    "start:nodemon": "nodemon --watch dev --exec 'npm run startquiet'",
    "lint:css": "stylelint **/*.scss",
    "lint:js": "eslint src --ext js --ext jsx",
    "test": "npm run lint:js && npm run lint:css && npm run test:js",
    "test:js": "jest",
    "test:js:snapshot": "jest -- --testPathPattern '\\.snapshots\\.test\\.js'",
    "test:js:unit": "jest -- --testPathPattern '^(.(?!\\.snapshots\\.test\\.js$))+$'",
    "test:update": "jest -u",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "jest": {
    "testRegex": "__tests__/.*[a-zA-Z-]+\\.test.js$",
    "collectCoverage": false,
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/index.{js,jsx}",
      "!**/__tests-config__/**",
      "!**/__tests__/**"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/src/__mocks__/setup-tests.js",
    "coveragePathIgnorePatterns": [
      "/__tests__/"
    ],
    "coverageDirectory": "coverage/",
    "coverageReporters": [
      "json",
      "lcov",
      "html",
      "text",
      "text-summary"
    ],
    "rootDir": ".",
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "moduleNameMapper": {
      "\\.(css|scss)$": "<rootDir>/src/__mocks__/style-mock.js"
    }
  },
  "devDependencies": {
    "babel": "6.3.26",
    "babel-cli": "6.26.0",
    "babel-core": "6.23.1",
    "babel-loader": "7.0.0",
    "babel-plugin-transform-amd-to-commonjs": "0.2.0",
    "babel-plugin-transform-es2015-destructuring": "6.23.0",
    "babel-plugin-transform-es2015-parameters": "6.23.0",
    "babel-polyfill": "6.23.0",
    "babel-preset-airbnb": "1.1.1",
    "babel-preset-env": "1.1.8",
    "babel-preset-es2015": "6.22.0",
    "babel-preset-react": "6.23.0",
    "babel-preset-stage-1": "6.22.0",
    "bootstrap": "4.3.1",
    "classnames": "2.2.6",
    "core-js": "2.4.1",
    "css-loader": "0.26.1",
    "enzyme": "3.8.0",
    "enzyme-adapter-react-16": "1.7.1",
    "enzyme-to-json": "3.3.5",
    "eslint": "5.12.1",
    "freemarker.js": "1.3.1",
    "immutable": "3.8.1",
    "jest": "23.6.0",
    "lodash": "4.17.13",
    "node-sass": "^4.12.0",
    "nodemon": "1.18.9",
    "prop-types": "15.6.2",
    "react": "16.8.6",
    "react-dom": "16.8.6",
    "react-immutable-proptypes": "2.1.0",
    "react-redux": "5.0.5",
    "react-responsive-carousel": "3.1.49",
    "react-router": "3.2.0",
    "react-router-redux": "4.0.8",
    "react-scripts": "3.0.1",
    "redux": "3.5.2",
    "redux-logger": "2.8.1",
    "redux-mock-store": "1.3.0",
    "redux-thunk": "2.2.0",
    "sass-loader": "7.1.0",
    "style-loader": "0.13.1",
    "stylelint": "9.10.1",
    "stylelint-config-standard": "18.2.0",
    "uglifyjs-webpack-plugin": "2.1.0",
    "webpack": "4.28.3",
    "webpack-cli": "3.2.0",
    "webpack-dev-server": "3.1.4",
    "whatwg-fetch": "2.0.2"
  }
}
